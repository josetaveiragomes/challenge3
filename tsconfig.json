{
    "compilerOptions": {
        "outDir": "./.tsbuild/",
        "sourceMap": false,
        "target": "es2019",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "strictPropertyInitialization": true,
        "strictNullChecks": true,
        "lib": ["es6"],
        "types": [
            "node", 
            "webdriverio/sync", 
            "@wdio/mocha-framework",
        ]
    }
}